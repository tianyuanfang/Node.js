{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {/*
        "indent": [
            "error",
           2,
           {"SwitchCase":1 ,"VariableDeclarator": { "var": 2, "let": 2, "const": 3  }  }
        ],*/
        "linebreak-style": [
            "error",
            "unix"
        ],/*
        "quotes": [
            "error"
        ],
        "semi": [
            "error",
            "always"
        ],*/
        "no-console":["off"],
        "no-undef":["off"],
        "semi-style": ["off"]

    }
}
